;AAWA,IAAA,GAXA,iBAWA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,eAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAJA;AAKA;AAjBA;AATA,CAAA","sourcesContent":["\n<template>\n   <v-form @submit.prevent=\"submit\">\n        <div class=\"form-group\">\n            <label for=\"idea\">Ваша идея</label>\n            <v-input type=\"text\" class=\"form-control\" name=\"idea\" id=\"idea\" v-model=\"fields.name\" />\n            <div v-if=\"errors && errors.name\" class=\"text-danger\">{{ errors.name[0] }}</div>\n        </div>\n        \n        <v-button type=\"submit\" class=\"btn btn-primary\">Отправить идею!</v-button>\n        <v-button @click=\"get\">Получить идеи</v-button>\n        <div>{{ideas}}</div>\n    </v-form>\n</template>\n\n\n<script>\nimport axios from 'axios'\n\nexport default {\n\n  data() {\n    return {\n      fields: {},\n      errors: {},\n      ideas: {}\n    }\n  },\n  methods: {\n    submit() {\n      this.errors = {};\n      axios.post('api/idea/add', this.fields).then(() => {\n        alert('Идея отправлена!');\n      }).catch(error => {\n        if (error.response.status === 422) {\n          this.errors = error.response.data.errors || {};\n        }\n      });\n    },\n    get() {\n      axios.get('/api/idea/all')\n      .then((response) => {\n        console.log('get')\n        this.ideas = response.data\n      })\n    }\n  },\n}\n</script>\n"],"sourceRoot":"src/components"}]}